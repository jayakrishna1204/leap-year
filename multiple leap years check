#include <iostream>
using namespace std;

bool isLeapYear(int year) {
    return (year % 4 == 0 && year % 100 != 0) || (year % 400 == 0);
}

int main() {
    int startYear, endYear;
    
    cout << "Enter Start Year: ";
    cin >> startYear;
    cout << "Enter End Year: ";
    cin >> endYear;
    
    cout << "\nLeap Years Between " << startYear << " and " << endYear << ":\n";
    
    for (int year = startYear; year <= endYear; year++) {
        if (isLeapYear(year)) {
            cout << year << " ";
        }
    }
    cout << endl;
    
    return 0;
}
